object Service "<%= @service_name -%>" {
  host_name = "<%= @host_name -%>"
  check_command = "<%= @check_command -%>"
  check_interval = <%= @check_interval %>
  retry_interval = <%= @retry_interval %>
<%- if @s_groups -%>
  groups = <%= @s_groups %>
<% end -%>
<% @vars.each do |key, val| -%>
  vars.<%= key %> = "<%= val -%>"
<% end -%>
  vars.sla = "<%= @sla -%>"
}
